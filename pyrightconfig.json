    "useLibraryCodeForTypes": false,
    "autoImportCompletions": true,
    "indexing": false,
    "autoSearchPaths": false,
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.11",
            "pythonPlatform": "Linux",
            "extraPaths": [
                "."
