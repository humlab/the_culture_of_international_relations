{
    "include": [
        "common",
        "1_quantitative_analysis",
        "2_network_analysis", 
        "3_text_analysis",
        "0_bens_prioritized_methods"
    ],
    "exclude": [
        ".*",
        ".venv/**",
        "venv/**",
        "env/**",
        "deprecated",
        "other", 
        "documents",
        "backup",
        "temp",
        "__pycache__",
        "*.pyc",
        "*.svg",
        "node_modules",
        ".git",
        ".vscode",
        "*.egg-info",
        "build",
        "dist"
    ],
    "ignore": [
        ".venv/**/*.py",
        "venv/**/*.py",
        "env/**/*.py",
        "deprecated/**/*.py",
        "other/**/*.py", 
